syntax enable
colorscheme atom-dark-256
set tabstop=4
set softtabstop=4
set expandtab
set list
set listchars=space:.
set number
set showcmd
set cursorline
filetype indent on
set wildmenu
set lazyredraw
set showmatch
set incsearch
set hlsearch
set foldenable
set foldlevelstart=10
set foldnestmax=10
nnoremap <space> za
set foldmethod=indent
nnoremap <leader>u :GundoToggle<CR>

